<aside aria-labelledby="skills" class="">


	<h2 class="h2 text-center" id="skills">Skill Sets</h2>


	{% for item in skills %}
	<h3 id="{{ item.title | slugify }}" class="">{{ item.title }}</h3>
	<p>{{ item.description }}</p>
	<ul aria-labelledby="{{ item.title | slugify }}" class="">
		{% for listItem in item.listItems %}
		<li class="">{{ listItem }}</li>
		{% endfor %}
	</ul>
	{% endfor %}


</aside>
