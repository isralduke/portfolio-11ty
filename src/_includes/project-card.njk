<section class="" aria-labelledby="{{ item.data.title | slugify }}">
	<img src="/images/projects/{{ item.data.title | slugify }}/feature.png" alt="Preview image for {{ item.data.title }}." class="">
	<h2 id="{{ item.data.title | slugify }}" class="">{{ item.data.title }}</h2>
	<ul class="">
		{% for stat in item.data.stats %}
		<li class="">{{ stat.label }} {{ stat.value }}</li>
		{% endfor %}
	</ul>
	<a href="{{ item.url }}" class="">{{ item.data.title }}</a>
</section>
